<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wl.travel.dao.UserDao">


    <select id="selectById" resultType="com.wl.travel.entity.UserEntity">
        SELECT username as name, age FROM user where user_id = #{id}
    </select>
</mapper>